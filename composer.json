{
    "name": "orkhanahmadov/laravel-az-sms-sender",
    "description": "Laravel 5 SMS Sender for Azerbaijani SMS providers",
    "type": "library",
    "homepage": "https://github.com/orkhanahmadov/laravel-az-sms-sender",
    "keywords": ["azerbaijan", "sms api", "msm.az", "mobis.az"],
    "license": "MIT",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\LaravelAzSmsSender\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orkhanahmadov\\LaravelAzSmsSender\\LaravelAzSmsSenderServiceProvider"
            ],

            "aliases": {
                "SmsSender": "Orkhanahmadov\\LaravelAzSmsSender\\Facade\\SmsSender"
            }
        }
    }
}
